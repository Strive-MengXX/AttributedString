GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/GONMarkupParser" "${PODS_ROOT}/Headers/Public/NSString+Color"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/GONMarkupParser" -isystem "${PODS_ROOT}/Headers/Public/NSString+Color"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-GONMarkupParser" -l"Pods-NSString+Color" -framework "CoreText"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods